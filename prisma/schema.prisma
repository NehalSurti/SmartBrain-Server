generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  entries   Int      @default(0)
  createdAt DateTime @default(now()) @map("created_at")

  @@map("users")
}
